#!/usr/bin/env node
var which = require("../")
if (process.argv.length < 3) {
  console.error('Usage: which <program>...')
  process.exit(1)
}

process.exit(process.argv.slice(2).reduce(function(pv, current) {
  try {
    console.log(which.sync(current))
    return pv;
  } catch (e) {
    return 1;
  }
}, 0))
